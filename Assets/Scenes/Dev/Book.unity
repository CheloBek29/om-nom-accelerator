    \u043F\u043E\u044D\u0442\u0438\u0447\u0435\u0441\u043A\u0438\u043C \u043E\u0433\u043D\u0435\u043C,\r\n\u041D\u0438
    \u043E\u0441\u0442\u0440\u043E\u0442\u043E\u044E, \u043D\u0438 \u0443\u043C\u043E\u043C,\r\n\u041D\u0438
    \u043E\u0431\u0449\u0435\u0436\u0438\u0442\u0438\u044F \u0438\u0441\u043A\u0443\u0441\u0441\u0442\u0432\u043E\u043C;\r\n\u041D\u043E
    \u0440\u0430\u0437\u0433\u043E\u0432\u043E\u0440 \u0438\u0445 \u043C\u0438\u043B\u044B\u0445
    \u0436\u0435\u043D\r\n\u0413\u043E\u0440\u0430\u0437\u0434\u043E \u043C\u0435\u043D\u044C\u0448\u0435
    \u0431\u044B\u043B \u0443\u043C\u0435\u043D.\r\n\r\nXII\r\n\u0411\u043E\u0433\u0430\u0442,
    \u0445\u043E\u0440\u043E\u0448 \u0441\u043E\u0431\u043E\u044E, \u041B\u0435\u043D\u0441\u043A\u0438\u0439\r\n\u0412\u0435\u0437\u0434\u0435
