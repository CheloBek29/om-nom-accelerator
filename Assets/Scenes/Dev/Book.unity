    \u043C\u0435\u0436 \u0441\u043E\u0431\u043E\u0439.\r\n\u0421\u043F\u0435\u0440\u0432\u0430
    \u0432\u0437\u0430\u0438\u043C\u043D\u043E\u0439 \u0440\u0430\u0437\u043D\u043E\u0442\u043E\u0439\r\n\u041E\u043D\u0438
    \u0434\u0440\u0443\u0433 \u0434\u0440\u0443\u0433\u0443 \u0431\u044B\u043B\u0438
    \u0441\u043A\u0443\u0447\u043D\u044B;\r\n\u041F\u043E\u0442\u043E\u043C \u043F\u043E\u043D\u0440\u0430\u0432\u0438\u043B\u0438\u0441\u044C;
    \u043F\u043E\u0442\u043E\u043C\r\n\u0421\u044A\u0435\u0437\u0436\u0430\u043B\u0438\u0441\u044C
    \u043A\u0430\u0436\u0434\u044B\u0439 \u0434\u0435\u043D\u044C \u0432\u0435\u0440\u0445\u043E\u043C\r\n\u0418
    \u0441\u043A\u043E\u0440\u043E \u0441\u0442\u0430\u043B\u0438 \u043D\u0435\u0440\u0430\u0437\u043B\u0443\u0447\u043D\u044B.\r\n\u0422\u0430\u043A
